edRule\"\n              />\n              <v-text-field\n                label=\"Sigla\"\n                name=\"sigla\"\n                type=\"text\"\n                v-model=\"cursoToEdit.sigla\"\n                required\n                :rules=\"requiredRule\"\n              />\n              <v-text-field\n                label=\"Turno\"\n                name=\"turno\"\n                type=\"text\"\n                v-model=\"cursoToEdit.turno\"\n                required\n                :rules=\"requiredRule\"\n              />\n            </v-card-text>\n            <v-card-actions>\n              <v-spacer />\n              <v-btn color=\"primary\" :loading=\"loading\" type=\"submit\" large>\n                Editar\n              </v-btn>\n            </v-card-actions>\n          </v-form>\n        </v-card>\n      </v-col>\n    </v-row>\n  </v-container>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  props: [\"curso\", \"editDisabled\"],\n  data: () => ({\n    valid: true,\n    loading: false,\n    cursoToEdit: {\n      nome: \"\",\n      sigla: \"\",\n      turno: \"\",\n    },\n    error: \"\",\n    requiredRule: [(v) => !!v || \"Campo obrigat√≥rio\"],\n  }),\n  created() {\n    this.cursoToEdit = this.curso;\n  },\n\n  methods: {\n    async editarCurso() {\n      await axios.put(`/cursos/${this.cursoToEdit.id}`, {\n        id: this.cursoToEdit.id,\n        nome: this.cursoToEdit.nome,\n        sigla: this.cursoToEdit.sigla,\n        turno: this.cursoToEdit.turno,\n      });\n      this.$emit(\"update:editDisabled\", true);\n    },\n  },\n};\n</script>\n\n<style scoped lang=\"css\">\n</style>"]}]}