                        <v-text-field label=\"Turno\" name=\"turno\" prepend-icon=\"\" type=\"text\" v-model=\"turno\" required :rules=\"requiredRule\"/>\n                            \n                        </v-card-text>\n                        <v-card-actions>\n                            <v-spacer />\n                            <v-btn color=\"primary\" :loading=\"loading\" type=\"submit\" large>Criar</v-btn>\n                        </v-card-actions>\n\n                    </v-form>\n                </v-card>\n            </v-col>\n        </v-row>\n    </v-container>\n</template>\n\n<script>\n\nimport axios from 'axios';\n\n    export default {\n        props: [\"to\", \"invalidToken\"],\n        data: () => ({\n            valid: true,\n            loading: false,\n            nome: '',\n            sigla: '',\n            turno: '',\n            error: '',\n            requiredRule: [\n                v => !!v || 'Campo obrigatÃ³rio'\n            ],\n        }),\n        methods: {\n            criarCurso: function() {    \n                if (this.id == '' || this.id == null) {\n                    axios\n                        .post(\"/cursos\", {\n                            \"id\": this.id,\n                            \"nome\": this.nome,\n                            \"sigla\": this.sigla,\n                            \"turno\": this.turno\n                        })\n                        .then(savedCurso => {\n                            this.cursos.push(savedCurso.data);\n                            this.cursos.nome = '';\n                            this.cursos.sigla = '';\n                            this.cursos.turno = '';\n                        })\n                    this.$router.push('/cursos')\n                }           \n                \n            }\n        }\n    };\n</script>\n\n<style scoped lang=\"css\">\n\n</style>"]}]}