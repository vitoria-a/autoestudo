Factory.get(\"matricula\");\n\nexport default {\n    data: () => ({\n      headers: [\n        {\n          text: \"ID Aluno\",\n          align: \"left\",\n          value: \"id_user\"\n        },\n        {\n          text: \"ID Curso\",\n          align: \"left\",\n          value: \"id_curso\"\n        }\n      ],\n      matriculas: []\n\n    }),\n\n    created() {\n        matriculaRepo\n        .getAll()\n        .then((res) => {\n            this.matriculas = res.data;\n        })\n        .catch(console.error);\n\n    },\n};\n</script>"]}]}